{
  "name": "@apart-re/notify",
  "version": "1.0.5",
  "description": "Notify",
  "repository": {
    "type": "git",
    "url": "git://github.com/apart-re/monorepo.git",
    "directory": "packages/notify"
  },
  "license": "MIT",
  "author": "Oswaldo Tedesco <oswaldo.tedesco@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "private": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "jest": {
    "preset": "@apart-re/jest-presets/node"
  },
  "packageManager": "pnpm@8.3.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@apart-re/config-typescript": "workspace:*",
    "@apart-re/eslint-config": "workspace:*",
    "@apart-re/jest-presets": "workspace:*",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@apart-re/commons": "workspace:^",
    "@azure/service-bus": "^7.9.5",
    "bluebird": "^3.7.2",
    "dotenv": "^16.4.5",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "redis": "^4.6.15"
  }
}
