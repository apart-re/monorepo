{
  "name": "@apart-re/server-utils",
  "version": "1.1.2",
  "description": "Server-utils",
  "repository": {
    "type": "git",
    "url": "git://github.com/apart-re/monorepo.git",
    "directory": "packages/server-utils"
  },
  "license": "MIT",
  "author": "Oswaldo Tedesco <oswaldo.tedesco@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "private": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "jest": {
    "preset": "@apart-re/jest-presets/node"
  },
  "packageManager": "pnpm@8.3.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@apart-re/eslint-config": "workspace:*",
    "@apart-re/jest-presets": "workspace:*",
    "@apart-re/typescript-config": "workspace:*",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-list-routes": "^1.2.2",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0"
  }
}